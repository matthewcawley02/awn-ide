{
  "name": "awn-syntax-highlighter",
  "displayName": "awn-syntax-highlighter",
  "description": "Syntax highlighting for the AWN programming language.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "awn",
      "aliases": ["AWN", "awn"],
      "extensions": [".awn"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "awn",
      "scopeName": "source.awn",
      "path": "./syntaxes/awn.tmLanguage.json"
    }]
  }
}
