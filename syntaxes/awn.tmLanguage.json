{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "awn",
	"scopeName": "source.awn",
	"patterns": [{ "include": "#strings" }],
	"repository": {
		"strings": {
			"patterns": [{ "include": "#keyword"}, {"include": "#infix_string"}, {"include": "#other_string"}, {"include": "name_string"}]
		}
	},
	"keyword": {
		"match": "forall|exists|lambda|\\.|include|proc|INCLUDES|TYPES|VARIABLES|CONSTANTS|FUNCTIONS|PREDICATES|PROCESSES|ALIASES",
		"name": "strings.keyword"
	},
	"infix_string": {
		"match": "[*+-:<=>!&|]+",
		"name": "strings.infix"
	},
	"other_string": {
		"match": "{|\\(|\\)|}|,|;",
		"name": "strings.other"
	},
	"name_string": {
		"match": "[a-zA-Z0-9\\.\\_\"#$%'/?@\\\\^~]+",
		"name": "strings.name"
	}
}